  </configSections>
  <appSettings file="AppSettings.config">
    <add key="webpages:Version" value="3.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="RepositoryCacheItemDuration" value="600"/>
  </appSettings>
  <connectionStrings configSource="ConnectionStrings.config"/>
  <system.web>
    <compilation targetFramework="4.6"/>
    <httpRuntime targetFramework="4.6" relaxedUrlToFileSystemMapping="true" requestValidationMode="2.0" maxUrlLength="512"/>
    <caching>
      <outputCacheSettings>
        <outputCacheProfiles>
          <add name="Default" duration="600" varyByParam="none" varyByCustom="User" location="Server"/> 
        </outputCacheProfiles>
      </outputCacheSettings>
    </caching>
    <httpModules>
      <add name="Glimpse" type="Glimpse.AspNet.HttpModule, Glimpse.AspNet"/>
    </httpModules>
    <httpHandlers>
      <add path="glimpse.axd" verb="GET" type="Glimpse.AspNet.HttpHandler, Glimpse.AspNet"/>
    </httpHandlers>
  </system.web>
  <system.webServer>
    <modules runAllManagedModulesForAllRequests="true">
      <add name="CMSApplicationModule" preCondition="managedHandler" type="CMS.Base.ApplicationModule, CMS.Base"/>
      <add name="Glimpse" type="Glimpse.AspNet.HttpModule, Glimpse.AspNet" preCondition="integratedMode"/>
    </modules>
    <staticContent>
      <remove fileExtension=".woff"/>
      <mimeMap fileExtension=".woff" mimeType="application/font-woff"/>
    </staticContent>
    <security>
      <requestFiltering>
        <requestLimits maxUrl="512"/>
      </requestFiltering>
    </security>
    <validation validateIntegratedModeConfiguration="false"/>
    <handlers>
      <add name="Glimpse" path="glimpse.axd" verb="GET" type="Glimpse.AspNet.HttpHandler, Glimpse.AspNet" preCondition="integratedMode"/>
    </handlers>
  </system.webServer>
  <system.codedom>
